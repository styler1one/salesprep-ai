# Copy this file to .env.local and fill in your values

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url-here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# API
NEXT_PUBLIC_API_URL=http://localhost:8000

# Sentry (Error Tracking)
# Get your DSN from https://sentry.io
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_ORG=your-org-name
SENTRY_PROJECT=dealmotion-frontend
# Auth token for source map uploads (optional, for better stack traces)
SENTRY_AUTH_TOKEN=

# PostHog (Analytics)
# Get your key from https://posthog.com (or https://eu.posthog.com for EU)
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=https://eu.i.posthog.com

# Stripe (Payments)
# Donation link for free users (Stripe Payment Link)
NEXT_PUBLIC_STRIPE_DONATION_LINK=https://buy.stripe.com/xxx